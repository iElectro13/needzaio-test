      "type": "string",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "IsMilitar": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "TimeCreate": {
      "type": "date"
    },
    "IsTemporal": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "userDocuments": {
      "type": "hasMany",
